{
  "name": "kairo-expo",
  "version": "1.0.0",
  "description": "React Native scaffold project with Expo",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "android:release": "cd android && ./gradlew assembleRelease",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild --clean",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "quality": "pnpm typecheck && pnpm lint && pnpm format:check && pnpm test",
    "prepare": "husky",
    "doctor": "npx expo doctor"
  },
  "keywords": [
    "react-native",
    "expo",
    "typescript",
    "scaffold"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@hookform/resolvers": "^3.9.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "@sentry/react-native": "^6.10.0",
    "@tanstack/react-query": "^5.60.0",
    "axios": "^1.7.0",
    "expo": "~52.0.0",
    "expo-constants": "~17.0.0",
    "expo-localization": "~16.0.0",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.9",
    "i18next": "^24.0.0",
    "nativewind": "^4.1.0",
    "react": "18.3.1",
    "react-hook-form": "^7.54.0",
    "react-i18next": "^15.0.0",
    "react-native": "0.76.9",
    "react-native-css-interop": "^0.2.1",
    "react-native-mmkv": "^4.1.0",
    "react-native-nitro-modules": "^0.31.10",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "zod": "^3.24.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.2",
    "@types/react": "~18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-expo": "12.0.11",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^9.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.50.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.15.0",
  "pnpm": {
    "patchedDependencies": {
      "react-native-nitro-modules@0.31.10": "patches/react-native-nitro-modules@0.31.10.patch"
    }
  }
}
